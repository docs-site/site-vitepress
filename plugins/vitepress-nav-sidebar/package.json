{
  "name": "@docs-site/vitepress-nav-sidebar",
  "version": "1.0.0",
  "main": "dist/index.mjs",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "default": "./dist/types.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "dist/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "tsx": "^4.20.3",
    "unbuild": "^3.6.0",
    "vitepress": "^1.6.4"
  },
  "scripts": {
    "build": "unbuild",
    "build-stub": "unbuild --stub",
    "build:tsc": "tsc --project tsconfig.prod.json",
    "test": "glob -c \"tsx --test\" \"./test/**/*.test.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "A VitePress plugin for automatically generating navigation and sidebar data",
  "dependencies": {
    "glob": "^11.0.3"
  }
}
